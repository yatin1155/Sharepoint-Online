{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "lead-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/leadForm/LeadFormWebPart.js",
          "manifest": "./src/webparts/leadForm/LeadFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    
    
    "material": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/material-design-lite/1.3.0/material.js",
      "globalName": "material"
    },
    "select2Lib":{
      "path": "https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.6-rc.0/js/select2.js",
      "globalName": "select2Lib"
    },
    "jquery": {
      "path":"https://code.jquery.com/jquery-3.3.1.js",
      "globalName": "jquery"
    }
  },
  "localizedResources": {
    "LeadFormWebPartStrings": "lib/webparts/leadForm/loc/{locale}.js"
  }
}
