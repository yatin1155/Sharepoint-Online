{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/grid/GridWebPart.js",
          "manifest": "./src/webparts/grid/GridWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "https://code.jquery.com/jquery-3.3.1.js",
    "datatables": "https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js",
    "fixedColumns": "https://cdn.datatables.net/fixedcolumns/3.2.6/js/dataTables.fixedColumns.min.js"
  },
  "localizedResources": {
    "GridWebPartStrings": "lib/webparts/grid/loc/{locale}.js",
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js"
  }
}
